<nodes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <node name="get_values_start" title="get values">
    <type>standard</type>
    <condition>$command == "value"</condition>
    <context>
        <promoted_array>&lt;? new JsonArray() ?&gt;</promoted_array>
        <demoted_array>&lt;? new JsonArray() ?&gt;</demoted_array>
        <promoted_finished>false</promoted_finished>
        <demoted_finished>false</demoted_finished>
        <repeat>&lt;? output.text ?&gt;</repeat>
    </context>
    <output>
      <textValues>
        <values structure="listItem">Now I would like to ask you about your personal values when choosing different routes. This is the list of values that can be positively or negatively supported when choosing a route: &lt;?$value_list.join(', ')?&gt;. You can also say none or no value. Can I continue?
        </values>
        <selection_policy>sequential</selection_policy>
      </textValues>
    </output>    
    <digress_in>does_not_return</digress_in>
    <digress_out>allow_all</digress_out>
    <nodes>
        <node name="promoted_values" title="Promoted values">
            <condition>@yes_no:yes and $promoted_finished == "false"</condition>
            <context>
              <repeat>&lt;? output.text ?&gt;</repeat>
            </context>
                <output>
                    <textValues>
                      <values structure="listItem">What values from the list would generally be positively supported when you choose route: &lt;?$value?&gt; to: &lt;?$concret?&gt;?</values>
                      <selection_policy>sequential</selection_policy>
                    </textValues>
                </output>    
            <nodes>
                <node name="does_not_know_promoted" title="None">
                  <type>standard</type>
                  <condition>#does_not_know || @silence || #LIST_OF_VALUES</condition>
                  <context>
                    <repeat>&lt;? output.text ?&gt;</repeat>
                  </context>
                  <output>
                    <textValues>
                      <values structure="listItem">You can choose values in this list: &lt;?$value_list.join(' ,')?&gt;. You can also say none or no value.
                        </values>
                      <selection_policy>sequential</selection_policy>
                    </textValues>
                  </output>
                  <goto>
                    <behavior>jump_to</behavior>
                    <target>get_values_start</target>
                    <selector>body</selector>
                  </goto>
                  <metadata structure="emptyDict"/>
                </node>
                <node name="no_promoted_values" title="No promoted values (noValues)">
